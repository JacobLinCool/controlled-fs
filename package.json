{
	"name": "controlled-fs",
	"version": "2.3.0",
	"description": "A type-safe filesystem abstraction for Node.js, with runtime validation and customizable serialization.",
	"keywords": [
		"fs",
		"filesystem",
		"file",
		"directory",
		"serialization",
		"validation"
	],
	"author": "JacobLinCool <jacoblincool@gmail.com> (https://github.com/JacobLinCool)",
	"license": "MIT",
	"main": "lib/index.js",
	"module": "lib/index.mjs",
	"types": "lib/index.d.ts",
	"files": [
		"lib"
	],
	"scripts": {
		"prepare": "husky install",
		"format": "prettier --write . --ignore-path .gitignore",
		"build": "tsup",
		"build:docs": "typedoc --out docs src",
		"test": "jest --coverage"
	},
	"dependencies": {
		"debug": "^4.3.4",
		"zod": "^3.20.2"
	},
	"devDependencies": {
		"@types/debug": "^4.1.7",
		"@types/jest": "^29.2.4",
		"@types/node": "^18.11.18",
		"husky": "^8.0.2",
		"jest": "^29.3.1",
		"lint-staged": "^13.1.0",
		"prettier": "^2.8.1",
		"serve": "^14.1.2",
		"ts-jest": "^29.0.3",
		"tsup": "^6.5.0",
		"tsx": "^3.12.1",
		"typedoc": "^0.23.23",
		"typescript": "^4.9.4"
	},
	"lint-staged": {
		"*.{ts,js,json,yaml,yml}": [
			"prettier --write"
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/JacobLinCool/controlled-fs.git"
	},
	"bugs": {
		"url": "https://github.com/JacobLinCool/controlled-fs/issues"
	},
	"homepage": "https://jacoblincool.github.io/controlled-fs"
}
